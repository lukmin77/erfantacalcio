{
  "name": "erfantacalcio.next-mui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "format": "prettier --write .",
    "build:ts": "tsc -p tsconfig.typeorm.json",
    "migration:create": "npm run build:ts && cross-env node ./node_modules/typeorm/cli.js -d dist/data-source.js migration:create",
    "migration:generate": "npm run build:ts && cross-env node ./node_modules/typeorm/cli.js -d dist/data-source.js migration:generate",
    "migration:run": "npm run build:ts && cross-env node ./node_modules/typeorm/cli.js -d dist/data-source.js migration:run",
    "migration:revert": "npm run build:ts && cross-env node ./node_modules/typeorm/cli.js -d dist/data-source.js migration:revert",
    "migration:show": "npm run build:ts && cross-env node ./node_modules/typeorm/cli.js -d dist/data-source.js migration:show",
    "migration:create:local": "cross-env DOTENV_CONFIG_PATH=.env npm run migration:create",
    "migration:generate:local": "cross-env DOTENV_CONFIG_PATH=.env npm run migration:generate",
    "migration:run:local": "cross-env DOTENV_CONFIG_PATH=.env npm run migration:run",
    "migration:revert:local": "cross-env DOTENV_CONFIG_PATH=.env npm run migration:revert",
    "migration:show:local": "cross-env DOTENV_CONFIG_PATH=.env npm run migration:show",
    "migration:create:prod": "cross-env DOTENV_CONFIG_PATH=.env.prod npm run migration:create",
    "migration:generate:prod": "cross-env DOTENV_CONFIG_PATH=.env.prod npm run migration:generate",
    "migration:run:prod": "cross-env DOTENV_CONFIG_PATH=.env.prod npm run migration:run",
    "migration:revert:prod": "cross-env DOTENV_CONFIG_PATH=.env.prod npm run migration:revert",
    "migration:show:prod": "cross-env DOTENV_CONFIG_PATH=.env.prod npm run migration:show"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-charts": "^8.22.0",
    "@mui/x-data-grid": "^8.22.0",
    "@mui/x-date-pickers": "^8.22.0",
    "@t3-oss/env-nextjs": "^0.7.3",
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@vercel/blob": "^0.23.4",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.6.0",
    "dayjs": "^1.11.19",
    "lodash": "^4.17.21",
    "next": "^16.0.8",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "reflect-metadata": "^0.1.14",
    "resend": "^3.5.0",
    "superjson": "^2.2.6",
    "typeorm": "^0.3.28",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.21",
    "@types/node": "^18.19.130",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.5.9",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.26.0"
  },
  "packageManager": "npm@10.2.3"
}
